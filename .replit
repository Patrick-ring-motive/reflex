entrypoint = "reflex.go"
run = ["./bin/build.sh"]

modules = ["go-1.24"]

[env]
GOPATH = "$REPL_HOME/gopath"

[nix]
channel = "stable-24_05"

[gitHubImport]
requiredFiles = [".replit"]

[deployment]
build = "CGO_ENABLED=0 GOOS=linux GOARCH=amd64 go build -a -o reflex.bin"
run = "./bin/build.sh"
ignorePorts = false
deploymentTarget = "gce"
